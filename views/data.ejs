<table>
    <thead>
        <tr>
            <th>Patient ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Date Of Birth</th>
            <th>Provider Specialty</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row=> { %>
            <tr>
                <td>
                    <%= row.patient_id %>
                </td>
                <td>
                    <%= row.first_name %>
                </td>
                <td>
                    <%= row.last_name %>
                </td>
                <td>
                    <%= row.date_of_birth ? row.date_of_birth.toISOString().split('T')[0] : 'N/A' %>
                </td>
                <td>
                    <%= row.provider_specialty %>
                </td>
                

            </tr>
            <% }); %>
    </tbody>
</table>